{
  "name": "nbsv",
  "version": "0.1.6",
  "description": "Simularium Viewer Jupyter Widget ",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/simularium/nbsv",
  "bugs": {
    "url": "https://github.com/simularium/nbsv/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Megan Riel-Mehan",
    "email": "meganr@alleninstitute.org"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/simularium/nbsv"
  },
  "scripts": {
    "watch": "yarn run build -- --sourcemap=inline --watch",
    "build": "esbuild src/widget.tsx --bundle --minify --format=esm --outdir=./nbsv/static --asset-names=[name].[ext]",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aics/simularium-viewer": "file:../simularium-viewer",
    "@anywidget/react": "^0.0.8",
    "@jupyter-widgets/base": "^6.0.10",
    "antd": "^5.22.1",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.169.0",
    "esbuild": "0.24.0",
    "typescript": "^5.6.3"
  }
}
